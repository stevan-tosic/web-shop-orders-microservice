<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TPartnerSetting" table="t_partner_setting">
    <indexes>
      <index name="fk_partner_setting_dropshipping_partner_id" columns="dropshipping_partner_id"/>
      <index name="fk_partner_setting_provider_id" columns="provider_id"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="value" type="string" column="value" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clientId" type="string" column="client_id" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="partnerType" type="string" column="partner_type" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="dropshippingPartner" target-entity="TDropshippingPartners" fetch="LAZY">
      <join-columns>
        <join-column name="dropshipping_partner_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="provider" target-entity="TProviders" fetch="LAZY">
      <join-columns>
        <join-column name="provider_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
