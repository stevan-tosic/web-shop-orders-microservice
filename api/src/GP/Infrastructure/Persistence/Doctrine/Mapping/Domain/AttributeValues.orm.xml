<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\AttributeValues" table="attribute_values">
    <indexes>
      <index name="FK_t_attribute_t_attribute_type" columns="attribute_type_id"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="value0" type="string" column="value_0" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="value1" type="string" column="value_1" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="value2" type="string" column="value_2" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customValue" type="string" column="custom_value" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="customValue2" type="string" column="custom_value_2" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="maxLength" type="integer" column="max_length" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="attributeType" target-entity="AttributeValueTypes" fetch="LAZY">
      <join-columns>
        <join-column name="attribute_type_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
