<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\InfobarSchedules" table="infobar_schedules">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="infobarId" type="integer" column="infobar_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="showOnMobile" type="boolean" column="show_on_mobile" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="showOnDesktop" type="boolean" column="show_on_desktop" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="startDate" type="datetime" column="start_date" nullable="true"/>
    <field name="endDate" type="datetime" column="end_date" nullable="true"/>
  </entity>
</doctrine-mapping>
