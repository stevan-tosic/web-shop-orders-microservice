<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TPayment" table="t_payment">
    <indexes>
      <index name="article_id_idx" columns="article_id"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="reihenfolge" type="integer" column="reihenfolge" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="creditCheck" type="integer" column="credit_check" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="account" type="integer" column="account" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="datatrans" type="integer" column="datatrans" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="datatransMethod" type="string" column="datatrans_method" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isBill" type="boolean" column="is_bill" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <many-to-one field="article" target-entity="TArticles" fetch="LAZY">
      <join-columns>
        <join-column name="article_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
