<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\LogsCache" table="logs_cache">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="message" type="string" column="message" length="512" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sourceType" type="string" column="source_type" length="512" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sourceId" type="integer" column="source_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="itemType" type="string" column="item_type" length="512" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="itemId" type="integer" column="item_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="state" type="integer" column="state" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
  </entity>
</doctrine-mapping>
