<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\ArticleAccount" table="article_account">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="articleTypeId" type="integer" column="article_type_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="articleId" type="integer" column="article_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="channel" type="integer" column="channel" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="account" type="integer" column="account" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isPorto" type="integer" column="is_porto" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="retailer" type="integer" column="retailer" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
