<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\ApiLogs" table="api_logs">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="path" type="string" column="path" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="true"/>
    <field name="request" type="text" column="request" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="response" type="text" column="response" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="orderNumber" type="string" column="order_number" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="statusCode" type="string" column="status_code" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="apiAccount" target-entity="ApiAccounts" fetch="LAZY">
      <join-columns>
        <join-column name="api_account_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
