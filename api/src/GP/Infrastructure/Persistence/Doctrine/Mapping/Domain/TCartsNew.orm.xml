<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TCartsNew" table="carts_new">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="gpuserid" type="string" column="gpUserID" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="orderNumber" type="string" column="order_number" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="paymentType" type="integer" column="payment_type" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="customerCareMessage" type="text" column="customer_care_message" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="voucherTotal" type="float" column="voucher_total" precision="10" scale="0" nullable="true"/>
    <field name="expShippingCosts" type="float" column="exp_shipping_costs" precision="10" scale="0" nullable="true"/>
    <field name="prdShippingCosts" type="float" column="prd_shipping_costs" precision="10" scale="0" nullable="true"/>
    <field name="total" type="float" column="total" precision="10" scale="0" nullable="true"/>
    <field name="active" type="integer" column="active" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="creditWorthy" type="boolean" column="credit_worthy" nullable="true"/>
    <field name="billingAddressId" type="integer" column="billing_address_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="shippingAddressId" type="integer" column="shipping_address_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hostId" type="integer" column="host_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ipAddress" type="string" column="ip_address" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="browser" type="text" column="browser" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adminSettings" type="text" column="admin_settings" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="insuranceDesired" type="integer" column="insurance_desired" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
