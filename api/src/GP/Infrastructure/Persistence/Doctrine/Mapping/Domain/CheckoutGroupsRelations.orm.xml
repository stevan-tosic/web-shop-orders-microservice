<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\CheckoutGroupsRelations" table="checkout_groups_relations">

    <unique-constraints>
      <unique-constraint name="checkout_group_id_host_id_article_type_id" columns="checkout_group_id,host_id,article_type_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sortOrder" type="integer" column="sort_order" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="updatedBy" type="integer" column="updated_by" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <many-to-one field="createdBy" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="checkoutGroup" target-entity="CheckoutGroups" fetch="LAZY">
      <join-columns>
        <join-column name="checkout_group_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="host" target-entity="Hosts" fetch="LAZY">
      <join-columns>
        <join-column name="host_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="articleType" target-entity="ArticleType" fetch="LAZY">
      <join-columns>
        <join-column name="article_type_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="overrideArticle" target-entity="Articles" fetch="LAZY">
      <join-columns>
        <join-column name="override_article_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
