<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\ArticleLogs" table="article_logs">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="articleId" type="integer" column="article_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="data" type="string" column="data" length="1024" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comment" type="string" column="comment" length="1024" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="when" type="datetime" column="when" nullable="false"/>
    <field name="user" type="string" column="user" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="articleVerticalId" type="integer" column="article_vertical_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
