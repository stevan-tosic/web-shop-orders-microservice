<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TContactform" table="t_contactform">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="orderId" type="integer" column="order_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="emailaddress" type="string" column="emailaddress" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="message" type="text" column="message" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="website" type="string" column="website" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="true"/>
    <field name="hostId" type="integer" column="host_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="contactformSubject" target-entity="TContactformSubjects" fetch="LAZY">
      <join-columns>
        <join-column name="contactform_subject_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
