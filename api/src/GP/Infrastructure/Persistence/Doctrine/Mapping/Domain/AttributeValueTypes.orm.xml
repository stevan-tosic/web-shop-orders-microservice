<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\AttributeValueTypes" table="attribute_value_types">

    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title0" type="string" column="title_0" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title1" type="string" column="title_1" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title2" type="string" column="title_2" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="attributeTypeField" target-entity="AttributeTypeFields" fetch="LAZY">
      <join-columns>
        <join-column name="attribute_type_field_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
