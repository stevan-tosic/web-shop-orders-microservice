<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\GeoStates" table="geo_states">
    <indexes>
      <index name="geo_states_t_geo_regions_id_fk" columns="region_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name0" type="string" column="name_0" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name1" type="string" column="name_1" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name2" type="string" column="name_2" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="acronym" type="string" column="acronym" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="region" target-entity="GeoRegions" fetch="LAZY">
      <join-columns>
        <join-column name="region_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
