<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TVariationToTypeMap" table="variation_to_type_map">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="variationId" type="integer" column="variation_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="typeId" type="integer" column="type_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="variationDropdown" type="boolean" column="variation_dropdown" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="propertiesBox" type="boolean" column="properties_box" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
