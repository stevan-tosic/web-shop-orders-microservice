<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TLinklistsLinks" table="linklists_links">
    <indexes>
      <index name="FK__t_linklis__linkl__22DFFF17" columns="linklist_id"/>
      <index name="FK__t_linklis__permi__21EBDADE" columns="permission_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="text" type="string" column="text" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="url" type="string" column="url" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="target" type="string" column="target" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rel" type="string" column="rel" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sortOrder" type="integer" column="sort_order" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cssClass" type="string" column="css_class" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="color" type="string" column="color" length="6" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <many-to-one field="linklist" target-entity="TLinklists" fetch="LAZY">
      <join-columns>
        <join-column name="linklist_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="permission" target-entity="TPermissions" fetch="LAZY">
      <join-columns>
        <join-column name="permission_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
