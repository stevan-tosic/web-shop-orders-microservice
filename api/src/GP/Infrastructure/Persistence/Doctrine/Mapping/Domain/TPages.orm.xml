<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TPages" table="pages">
    <indexes>
      <index name="pages_host_group_id_index" columns="host_group_id"/>
      <index name="pages_page_type_id_index" columns="page_type_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pageTypeId" type="integer" column="page_type_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hostGroupId" type="integer" column="host_group_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="isHomepage" type="boolean" column="is_homepage" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
