<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TArticleProducts" table="article_products">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ean" type="string" column="ean" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="brandId" type="integer" column="brand_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="brandArticleId" type="string" column="brand_article_id" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="distributorId" type="integer" column="distributor_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dropshipperId" type="integer" column="dropshipper_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="externalArticleId" type="string" column="external_article_id" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="externalTitle" type="string" column="external_title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="stock" type="integer" column="stock" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="minimumStock" type="integer" column="minimum_stock" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="stockReserved" type="integer" column="stock_reserved" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="acquisitionPrice" type="decimal" column="acquisition_price" precision="10" scale="2" nullable="false"/>
    <field name="logisticsNote" type="string" column="logistics_note" length="1024" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="deliveryTimeId" type="integer" column="delivery_time_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="unitId" type="integer" column="unit_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="articleStatusId" type="integer" column="article_status_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="processingCosts" type="decimal" column="processing_costs" precision="10" scale="2" nullable="true"/>
    <field name="lastChangeDate" type="datetime" column="last_change_date" nullable="true"/>
		<one-to-one field="article" target-entity="TArticles" inversed-by="articleProduct" />
    <many-to-one field="lastChangeUser" target-entity="TUsers" fetch="LAZY">
      <join-columns>
        <join-column name="last_change_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
