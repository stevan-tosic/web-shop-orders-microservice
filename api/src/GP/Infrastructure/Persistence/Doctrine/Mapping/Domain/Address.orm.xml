<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\Address" table="address">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titleId" type="integer" column="title_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="company" type="string" column="company" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="firstname" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address1" type="string" column="address_1" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address2" type="string" column="address_2" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zip" type="string" column="zip" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="location" type="string" column="location" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countryId" type="integer" column="country_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="birthDate" type="datetime" column="birth_date" nullable="true"/>
    <field name="registrationDate" type="datetime" column="registration_date" nullable="true"/>
    <field name="iban" type="string" column="iban" length="34" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="uid" type="string" column="uid" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="poNumber" type="string" column="po_number" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="costCenter" type="string" column="cost_center" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="reference" type="string" column="reference" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="department" type="string" column="department" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="user" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
