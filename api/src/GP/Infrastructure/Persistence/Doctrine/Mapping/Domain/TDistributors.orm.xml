<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TDistributors" table="t_distributors">

    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="address1" type="string" column="address_1" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address2" type="string" column="address_2" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="integer" column="country" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="company" type="string" column="company" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="firstname" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="homepage" type="string" column="homepage" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notes" type="text" column="notes" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="text" column="phone" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="integer" column="title" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="zip" type="string" column="zip" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="importCostsShare" type="decimal" column="import_costs_share" precision="10" scale="2" nullable="true"/>
    <many-to-one field="address" target-entity="TAddress" fetch="LAZY">
      <join-columns>
        <join-column name="address_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="currency" target-entity="TCurrencies" fetch="LAZY">
      <join-columns>
        <join-column name="currency_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="responsiblePerson" target-entity="TUsers" fetch="LAZY">
      <join-columns>
        <join-column name="responsible_person_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
