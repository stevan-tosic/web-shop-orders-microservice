<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TCheckoutGroupsArticles" table="t_checkout_groups_articles">

    <unique-constraints>
      <unique-constraint name="checkout_group_id_article_id" columns="checkout_group_id,article_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sortOrder" type="integer" column="sort_order" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="default" type="boolean" column="default" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="defaultOrder" type="integer" column="default_order" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="createdBy" type="integer" column="created_by" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="updatedBy" type="integer" column="updated_by" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <many-to-one field="checkoutGroup" target-entity="TCheckoutGroups" fetch="LAZY">
      <join-columns>
        <join-column name="checkout_group_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="article" target-entity="TArticles" fetch="LAZY">
      <join-columns>
        <join-column name="article_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
