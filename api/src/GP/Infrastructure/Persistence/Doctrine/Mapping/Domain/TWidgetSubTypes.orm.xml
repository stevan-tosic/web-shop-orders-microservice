<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TWidgetSubTypes" table="t_widget_sub_types">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mainWidgetTypeId" type="integer" column="main_widget_type_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="isRequired" type="boolean" column="is_required" nullable="true"/>
    <field name="sortOrder" type="integer" column="sort_order" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subWidgetTypeId" type="integer" column="sub_widget_type_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
