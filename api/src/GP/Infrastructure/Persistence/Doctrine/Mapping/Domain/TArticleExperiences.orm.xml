<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TArticleExperiences" table="article_experiences">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="locationId" type="integer" column="location_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="isEverywhere" type="boolean" column="is_everywhere" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="people" type="integer" column="people" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="commissionOffline" type="float" column="commission_offline" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isOffline" type="boolean" column="is_offline" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isTicket" type="boolean" column="is_ticket" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="providerId" type="integer" column="provider_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="providerSubId" type="integer" column="provider_sub_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="boxIsVirtual" type="boolean" column="box_is_virtual" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="priceOffline" type="float" column="price_offline" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastChangeDate" type="datetime" column="last_change_date" nullable="true"/>
		<one-to-one field="article" target-entity="TArticles" inversed-by="articleExperience" />
    <many-to-one field="lastChangeUser" target-entity="TUsers" fetch="LAZY">
      <join-columns>
        <join-column name="last_change_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
