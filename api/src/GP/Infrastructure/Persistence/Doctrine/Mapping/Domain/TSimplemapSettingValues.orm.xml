        <join-column name="region_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="setting" target-entity="TSimplemapSettings" fetch="LAZY">
      <join-columns>
        <join-column name="setting_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="state" target-entity="TSimplemapStates" fetch="LAZY">
