<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TTaxes" table="t_taxes">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title0" type="string" column="title_0" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title1" type="string" column="title_1" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title2" type="string" column="title_2" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tax" type="float" column="tax" precision="10" scale="0" nullable="true"/>
    <field name="typeId" type="integer" column="type_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="expirationDate" type="datetime" column="expiration_date" nullable="true"/>
    <field name="label" type="string" column="label" length="55" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
