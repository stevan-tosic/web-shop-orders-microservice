<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TUnitTests" table="unit_tests">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="coverageReport" type="string" column="coverage_report" length="32" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="startedManually" type="boolean" column="started_manually" nullable="false"/>
    <field name="timestamp" type="datetime" column="timestamp" nullable="false"/>
    <field name="failedTests" type="integer" column="failed_tests" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="passedTests" type="integer" column="passed_tests" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="who" type="string" column="who" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
