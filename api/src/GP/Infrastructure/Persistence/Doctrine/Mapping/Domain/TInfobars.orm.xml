<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TInfobars" table="t_infobars">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="text" type="string" column="text" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="layout" type="string" column="layout" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="classId" type="integer" column="class_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="typeId" type="integer" column="type_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="creationDate" type="datetime" column="creation_date" nullable="true"/>
    <field name="creationUserId" type="integer" column="creation_user_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lastChangeDate" type="datetime" column="last_change_date" nullable="true"/>
    <field name="lastChangeUserId" type="integer" column="last_change_user_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="showCloseButton" type="boolean" column="show_close_button" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="lang" type="integer" column="lang" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
