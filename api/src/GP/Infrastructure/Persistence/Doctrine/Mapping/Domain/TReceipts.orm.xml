<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TReceipts" table="t_receipts">
    <indexes>
      <index name="fk_receipts_receipt_accounts1_idx" columns="receipt_account_id"/>
      <index name="fk_receipts_receipt_type_idx" columns="receipt_type_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="staticText" type="string" column="static_text" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="decorateAction" type="string" column="decorate_action" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="receiptAccount" target-entity="TReceiptAccounts" fetch="LAZY">
      <join-columns>
        <join-column name="receipt_account_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="receiptType" target-entity="TReceiptTypes" fetch="LAZY">
      <join-columns>
        <join-column name="receipt_type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="step" target-entity="TDunningSteps" mapped-by="receipt" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
