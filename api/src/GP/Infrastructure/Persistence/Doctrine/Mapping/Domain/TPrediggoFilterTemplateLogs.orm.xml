<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TPrediggoFilterTemplateLogs" table="prediggo_filter_template_logs">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="contentBeforeChange" type="json" column="content_before_change" nullable="true"/>
    <field name="contentAfterChange" type="json" column="content_after_change" nullable="true"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <many-to-one field="filterTemplate" target-entity="TPrediggoFilterTemplates" fetch="LAZY">
      <join-columns>
        <join-column name="filter_template_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="updatedBy" target-entity="TUsers" fetch="LAZY">
      <join-columns>
        <join-column name="updated_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
