<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\MetaKeywords" table="meta_keywords">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="keyword" type="string" column="keyword" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="language" type="integer" column="language" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-many field="article" target-entity="Articles" mapped-by="keyword" fetch="LAZY"/>
    <many-to-many field="category" target-entity="CategoriesNew" mapped-by="keyword" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
