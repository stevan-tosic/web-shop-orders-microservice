        <option name="fixed"/>
      </options>
    </field>
    <field name="bankaccount" type="text" column="bankaccount" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comments" type="text" column="comments" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notes" type="text" column="notes" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="billingAddressId" type="integer" column="billing_address_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="paymentReference" type="string" column="payment_reference" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
