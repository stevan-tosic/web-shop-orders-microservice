      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zip" type="string" column="ZIP" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="location" type="string" column="location" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
