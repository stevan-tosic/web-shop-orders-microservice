<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\CheckoutGroups" table="checkout_groups">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="standalone" type="boolean" column="standalone" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="multiple" type="boolean" column="multiple" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mandatory" type="boolean" column="mandatory" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="skipChoice" type="boolean" column="skip_choice" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="minArticles" type="integer" column="min_articles" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">3</option>
      </options>
    </field>
    <field name="maxArticles" type="integer" column="max_articles" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">6</option>
      </options>
    </field>
    <field name="quantifiable" type="boolean" column="quantifiable" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="creditCheckRequired" type="boolean" column="credit_check_required" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="createdBy" type="integer" column="created_by" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="updatedBy" type="integer" column="updated_by" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <many-to-one field="description" target-entity="I18n" fetch="LAZY">
      <join-columns>
        <join-column name="description_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="title" target-entity="I18n" fetch="LAZY">
      <join-columns>
        <join-column name="title_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="category" target-entity="CheckoutGroupsCategories" fetch="LAZY">
      <join-columns>
        <join-column name="category_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
