<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\BankAccounts" table="bank_accounts">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bank" type="string" column="bank" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zip" type="string" column="zip" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="accountNumber" type="string" column="account_number" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="referenceNumber" type="string" column="reference_number" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="showBank" type="boolean" column="show_bank" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
