<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TCrons" table="t_crons">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="startDate" type="datetime" column="start_date" nullable="false"/>
    <field name="endDate" type="datetime" column="end_date" nullable="false"/>
    <field name="creationDate" type="datetime" column="creation_date" nullable="false"/>
    <field name="creationUserId" type="integer" column="creation_user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lastChangeDate" type="datetime" column="last_change_date" nullable="true"/>
    <field name="lastChangeUserId" type="integer" column="last_change_user_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="controller" type="string" column="controller" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="action" type="string" column="action" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cronPattern" type="string" column="cron_pattern" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
