<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TIntake" table="t_intake">

    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="distributorId" type="integer" column="distributor_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="modifiedDate" type="datetime" column="modified_date" nullable="true"/>
    <field name="orderDate" type="datetime" column="order_date" nullable="true"/>
    <field name="shipmentTracking" type="string" column="shipment_tracking" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="shipmentWeight" type="float" column="shipment_weight" precision="10" scale="0" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="comment" type="text" column="comment" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="shipmentDate" type="datetime" column="shipment_date" nullable="true"/>
  </entity>
</doctrine-mapping>
