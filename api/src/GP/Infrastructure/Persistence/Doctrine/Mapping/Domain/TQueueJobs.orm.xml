<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TQueueJobs" table="queue_jobs">
    <unique-constraints>
      <unique-constraint name="queue_jobs_uindex" columns="channel"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="channel" type="string" column="channel" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="class" type="string" column="class" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="method" type="string" column="method" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="false"/>
    <field name="platform" type="string" column="platform" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">Gearman</option>
      </options>
    </field>
    <field name="exchange" type="string" column="exchange" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="delay" type="integer" column="delay" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="parallel" type="integer" column="parallel" nullable="true">
      <options>
        <option name="unsigned">1</option>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
