<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\ExternalApis" table="external_apis">
    <indexes>
      <index name="external_api_host_group_idx" columns="host_group_id"/>
      <index name="external_api_partner_idx" columns="partner_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="externalId" type="string" column="external_id" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="token" type="string" column="token" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="hostGroup" target-entity="HostGroups" fetch="LAZY">
      <join-columns>
        <join-column name="host_group_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="partner" target-entity="ExternalApiPartners" fetch="LAZY">
      <join-columns>
        <join-column name="partner_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
