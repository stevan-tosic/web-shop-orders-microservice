<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TCategoriesNew" table="categories_new">
    <indexes>
      <index name="categories_new_category_group_id_index" columns="category_group_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="categoryGroupId" type="integer" column="category_group_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hostId" type="integer" column="host_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subtitle" type="string" column="subtitle" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="slug" type="string" column="slug" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="seoTextShort" type="text" column="seo_text_short" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="seoTextLong" type="text" column="seo_text_long" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="true"/>
    <field name="googleCategory" type="string" column="google_category" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="categoryStatusId" type="integer" column="category_status_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="redirectTarget" type="string" column="redirect_target" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="siroopCategory" type="string" column="siroop_category" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="keyword" target-entity="TMetaKeywords" inversed-by="category" fetch="LAZY">
      <join-table name="meta_keywords_to_categories_map">
        <join-columns>
          <join-column name="category_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="keyword_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
