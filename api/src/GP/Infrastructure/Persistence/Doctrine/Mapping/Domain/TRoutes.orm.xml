<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TRoutes" table="routes">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pattern" type="string" column="pattern" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="namespace" type="string" column="namespace" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="matcherId" type="integer" column="matcher_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dispatcherId" type="integer" column="dispatcher_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
