<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TQueueDelayedMessages" table="t_queue_delayed_messages">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="channel" type="string" column="channel" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="payload" type="text" column="payload" length="0" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="lastCheck" type="datetime" column="last_check" nullable="true"/>
    <field name="queued" type="datetime" column="queued" nullable="true"/>
    <field name="hash" type="string" column="hash" length="32" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
