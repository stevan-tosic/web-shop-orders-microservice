<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\JhiPersistentAuditEvtData" table="jhi_persistent_audit_evt_data">

    <id name="name" type="string" column="name" length="150"/>
    <id name="event" association-key="true"/>
    <field name="value" type="string" column="value" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="event" target-entity="JhiPersistentAuditEvent" fetch="LAZY">
      <join-columns>
        <join-column name="event_id" referenced-column-name="event_id"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
