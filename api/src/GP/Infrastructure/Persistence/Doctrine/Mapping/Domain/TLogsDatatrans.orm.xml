<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TLogsDatatrans" table="t_logs_datatrans">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="orderNumber" type="string" column="order_number" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="url" type="string" column="url" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="string" column="status" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hostId" type="integer" column="host_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="errorCode" type="integer" column="error_code" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
