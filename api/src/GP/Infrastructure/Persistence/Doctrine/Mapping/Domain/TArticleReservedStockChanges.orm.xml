<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TArticleReservedStockChanges" table="article_reserved_stock_changes">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="articleId" type="integer" column="article_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="orderId" type="integer" column="order_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="orderLineId" type="integer" column="order_line_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="llsFile" type="string" column="lls_file" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="reservedChange" type="integer" column="reserved_change" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="logDate" type="datetime" column="log_date" nullable="true"/>
    <field name="data" type="text" column="data" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
