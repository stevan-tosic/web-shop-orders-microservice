<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\IntakeLines" table="intake_lines">
    <indexes>
      <index name="IX_t_intake_lines_intake_id" columns="intake_id"/>
      <index name="IX_t_intake_lines_product_id" columns="product_variation_id"/>
      <index name="IX_t_intake_lines_status_id" columns="status_id"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="intakeId" type="integer" column="intake_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="productVariationId" type="integer" column="product_variation_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="statusId" type="integer" column="status_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="quantity" type="integer" column="quantity" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="comment" type="text" column="comment" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="deliveryDate" type="datetime" column="delivery_date" nullable="true"/>
    <field name="clearedDate" type="datetime" column="cleared_date" nullable="true"/>
    <field name="quantityDelivered" type="integer" column="quantity_delivered" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
