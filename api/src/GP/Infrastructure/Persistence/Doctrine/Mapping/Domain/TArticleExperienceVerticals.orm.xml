<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TArticleExperienceVerticals" table="article_experience_verticals">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="commission" type="float" column="commission" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="when" type="text" column="when" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="requirements" type="text" column="requirements" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="duration" type="text" column="duration" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="weather" type="text" column="weather" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="equipment" type="text" column="equipment" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="participants" type="text" column="participants" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="specials" type="text" column="specials" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="redeemNote" type="text" column="redeem_note" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="offlineText" type="text" column="offline_text" length="0" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="boxIsTextListing" type="boolean" column="box_is_text_listing" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastChangeDate" type="datetime" column="last_change_date" nullable="true"/>
    <many-to-one field="lastChangeUser" target-entity="TUsers" fetch="LAZY">
      <join-columns>
        <join-column name="last_change_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
