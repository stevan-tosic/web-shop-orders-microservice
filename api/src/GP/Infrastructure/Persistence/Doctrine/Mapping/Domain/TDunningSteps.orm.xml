<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TDunningSteps" table="dunning_steps">
    <indexes>
      <index name="cancellation_status_id" columns="cancellation_status_id"/>
      <index name="email_attachment_file_name_translation_id" columns="email_attachment_file_name_translation_id"/>
      <index name="email_subject_translation_id" columns="email_subject_translation_id"/>
      <index name="email_text_translation_id" columns="email_text_translation_id"/>
      <index name="email_title_translation_id" columns="email_title_translation_id"/>
      <index name="pdf_post_table_text_translation_id" columns="pdf_post_table_text_translation_id"/>
      <index name="pdf_pre_table_text_translation_id" columns="pdf_pre_table_text_translation_id"/>
      <index name="pdf_title_translation_id" columns="pdf_title_translation_id"/>
      <index name="sender_translation_id" columns="sender_translation_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="dunning_steps_label_uindex" columns="label"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="billingStatusDays" type="integer" column="billing_status_days" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="deactivateLines" type="boolean" column="deactivate_lines" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="autoRun" type="boolean" column="auto_run" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="belongToDunning" type="boolean" column="belong_to_dunning" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="isEmailWithAttachment" type="boolean" column="is_email_with_attachment" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="priority" type="integer" column="priority" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="label" type="string" column="label" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isUsingReceipts" type="boolean" column="is_using_receipts" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isAddingArticles" type="boolean" column="is_adding_articles" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isRemovingArticles" type="boolean" column="is_removing_articles" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="orderRelated" type="boolean" column="order_related" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <many-to-one field="targetBillingStatus" target-entity="TBillingStatus" fetch="LAZY">
      <join-columns>
        <join-column name="target_billing_status_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="senderTranslation" target-entity="TI18n" fetch="LAZY">
      <join-columns>
        <join-column name="sender_translation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="cancellationStatus" target-entity="TCancellationStatus" fetch="LAZY">
      <join-columns>
        <join-column name="cancellation_status_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="emailSubjectTranslation" target-entity="TI18n" fetch="LAZY">
      <join-columns>
        <join-column name="email_subject_translation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="emailTitleTranslation" target-entity="TI18n" fetch="LAZY">
      <join-columns>
        <join-column name="email_title_translation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="emailTextTranslation" target-entity="TI18n" fetch="LAZY">
      <join-columns>
        <join-column name="email_text_translation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="emailAttachmentFileNameTranslation" target-entity="TI18n" fetch="LAZY">
      <join-columns>
        <join-column name="email_attachment_file_name_translation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="pdfPreTableTextTranslation" target-entity="TI18n" fetch="LAZY">
      <join-columns>
        <join-column name="pdf_pre_table_text_translation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="pdfPostTableTextTranslation" target-entity="TI18n" fetch="LAZY">
      <join-columns>
        <join-column name="pdf_post_table_text_translation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="pdfTitleTranslation" target-entity="TI18n" fetch="LAZY">
      <join-columns>
        <join-column name="pdf_title_translation_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="billingStatus" target-entity="TBillingStatus" inversed-by="step" fetch="LAZY">
      <join-table name="dunning_step_billing_status_affected">
        <join-columns>
          <join-column name="step_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="billing_status_id" referenced-column-name="ID"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="correspondenceType" target-entity="TCorrespondenceTypes" inversed-by="step" fetch="LAZY">
      <join-table name="dunning_step_to_correspondence_type">
        <join-columns>
          <join-column name="step_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="correspondence_type_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="receipt" target-entity="TReceipts" inversed-by="step" fetch="LAZY">
      <join-table name="dunning_step_to_receipt">
        <join-columns>
          <join-column name="step_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="receipt_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="dunningType" target-entity="TDunningTypes" inversed-by="step" fetch="LAZY">
      <join-table name="dunning_step_to_type">
        <join-columns>
          <join-column name="step_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="dunning_type_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
