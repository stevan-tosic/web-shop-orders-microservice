<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TApiAccounts" table="t_api_accounts">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apiKey" type="string" column="api_key" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="true"/>
    <field name="creationDate" type="datetime" column="creation_date" nullable="false"/>
    <field name="lastUpdateDate" type="datetime" column="last_update_date" nullable="false"/>
    <many-to-one field="creationUser" target-entity="TUsers" fetch="LAZY">
      <join-columns>
        <join-column name="creation_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="responsibleUser" target-entity="TUsers" fetch="LAZY">
      <join-columns>
        <join-column name="responsible_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastUpdateUser" target-entity="TUsers" fetch="LAZY">
      <join-columns>
        <join-column name="last_update_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="contact" target-entity="TAddress" fetch="LAZY">
      <join-columns>
        <join-column name="contact_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
