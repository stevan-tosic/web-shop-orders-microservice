<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TUsers" table="t_users">
    <indexes>
      <index name="address_to_user" columns="address_id"/>
      <index name="dunning_allowed_idx" columns="dunning_allowed"/>
      <index name="user_rank_idx" columns="rank_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="false"/>
    <field name="dunningAllowed" type="boolean" column="dunning_allowed" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <many-to-one field="address" target-entity="TAddress" fetch="LAZY">
      <join-columns>
        <join-column name="address_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="rank" target-entity="TUserRanks" fetch="LAZY">
      <join-columns>
        <join-column name="rank_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="partner" target-entity="TDropshippingPartners" inversed-by="user" fetch="LAZY">
      <join-table name="t_dropshipping_user_to_partner_map">
        <join-columns>
          <join-column name="user_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="partner_id" referenced-column-name="ID"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="usergroup" target-entity="TUsergroups" inversed-by="user" fetch="LAZY">
      <join-table name="t_usergroup_to_user_map">
        <join-columns>
          <join-column name="user_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="usergroup_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
