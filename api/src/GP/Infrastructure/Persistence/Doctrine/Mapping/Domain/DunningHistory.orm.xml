<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\DunningHistory" table="dunning_history">
    <indexes>
      <index name="dunning_history_user_id_idx" columns="user_id"/>
      <index name="dunning_sources_id_fk" columns="dunning_source_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="datetime" column="date" nullable="true"/>
    <field name="isRunning" type="integer" column="is_running" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="printer" type="string" column="printer" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="runningUpdatedAt" type="datetime" column="running_updated_at" nullable="true"/>
    <many-to-one field="user" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="dunningSource" target-entity="DunningSources" fetch="LAZY">
      <join-columns>
        <join-column name="dunning_source_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
