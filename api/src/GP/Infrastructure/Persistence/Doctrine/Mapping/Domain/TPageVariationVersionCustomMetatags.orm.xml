<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TPageVariationVersionCustomMetatags" table="t_page_variation_version_custom_metatags">
    <indexes>
      <index name="fk_page_additional_metatags_version" columns="page_version_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="value" type="string" column="value" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="pageVersion" target-entity="TPageVariationVersions" fetch="LAZY">
      <join-columns>
        <join-column name="page_version_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
