<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TCartLines" table="cart_lines">

    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="gpuserid" type="string" column="gpUserID" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productid" type="integer" column="productID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="giftbox" type="integer" column="giftbox" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="receiver" type="string" column="receiver" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="price" type="float" column="price" precision="10" scale="0" nullable="true"/>
    <field name="commission" type="float" column="commission" precision="10" scale="0" nullable="true"/>
    <field name="product" type="integer" column="product" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="productVariationId" type="integer" column="product_variation_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quantity" type="integer" column="quantity" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="acquisitionPrice" type="float" column="acquisition_price" precision="10" scale="0" nullable="true"/>
    <field name="taxid" type="integer" column="taxID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="articleId" type="integer" column="article_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hostId" type="integer" column="host_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
    <field name="sourceCategoryTree" type="string" column="source_category_tree" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isStandaloneCheckoutArticle" type="boolean" column="is_standalone_checkout_article" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <many-to-one field="bundleLine" target-entity="TCartLines" fetch="LAZY">
      <join-columns>
        <join-column name="bundle_line_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="checkoutArticleParentLine" target-entity="TCartLines" fetch="LAZY">
      <join-columns>
        <join-column name="checkout_article_parent_line_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="checkoutGroup" target-entity="TCheckoutGroups" fetch="LAZY">
      <join-columns>
        <join-column name="checkout_group_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="cart" target-entity="TCarts" fetch="LAZY">
      <join-columns>
        <join-column name="cart_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sourceCategory" target-entity="TCategoriesNew" fetch="LAZY">
      <join-columns>
        <join-column name="source_category_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
