<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\THostGroupsPdfTemplates" table="t_host_groups_pdf_templates">
    <indexes>
      <index name="t_host_groups_pdf_templates_t_host_groups_id_fk" columns="host_group_id"/>
      <index name="t_host_groups_pdf_templates_t_pdf_templates_id_fk" columns="pdf_template_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="hostGroup" target-entity="THostGroups" fetch="LAZY">
      <join-columns>
        <join-column name="host_group_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="pdfTemplate" target-entity="TPdfTemplates" fetch="LAZY">
      <join-columns>
        <join-column name="pdf_template_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
