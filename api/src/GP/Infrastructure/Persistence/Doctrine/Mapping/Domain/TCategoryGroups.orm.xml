<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TCategoryGroups" table="t_category_groups">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="parentId" type="integer" column="parent_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hostGroupId" type="integer" column="host_group_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pageId" type="integer" column="page_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sortOrder" type="integer" column="sort_order" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="isGoogleShopping" type="boolean" column="is_google_shopping" nullable="true"/>
    <field name="isSiroop" type="boolean" column="is_siroop" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="totalRevenue" type="float" column="total_revenue" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="redeemedRevenue" type="float" column="redeemed_revenue" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="color" type="string" column="color" length="6" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <many-to-one field="filterTemplate" target-entity="TPrediggoFilterTemplates" fetch="LAZY">
      <join-columns>
        <join-column name="filter_template_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
