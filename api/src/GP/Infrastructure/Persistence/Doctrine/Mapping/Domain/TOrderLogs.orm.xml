<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TOrderLogs" table="order_logs">
    <indexes>
      <index name="fk_t_order_logs_order_line_id_t_order_lines_id" columns="order_line_id"/>
      <index name="fk_t_order_logs_partner_id_t_dropshipping_partners_id" columns="partner_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="orderId" type="integer" column="order_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="data" type="text" column="data" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comment" type="string" column="comment" length="1024" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="when" type="datetime" column="when" nullable="false"/>
    <field name="user" type="string" column="user" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isTopComment" type="boolean" column="is_top_comment" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="inactiveDate" type="datetime" column="inactive_date" nullable="true">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
    <many-to-one field="orderLine" target-entity="TOrderLines" fetch="LAZY">
      <join-columns>
        <join-column name="order_line_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="partner" target-entity="TDropshippingPartners" fetch="LAZY">
      <join-columns>
        <join-column name="partner_id" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
