<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\GP\Domain\Entity\TPageVariations" table="page_variations">

    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="creationDate" type="datetime" column="creation_date" nullable="true"/>
    <field name="creationUserId" type="integer" column="creation_user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lastChangeUserId" type="integer" column="last_change_user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lastChangeDate" type="datetime" column="last_change_date" nullable="true"/>
    <field name="activeVersionId" type="integer" column="active_version_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hostId" type="integer" column="host_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="slug" type="string" column="slug" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pageId" type="integer" column="page_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
